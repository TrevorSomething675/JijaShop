@using JijaShop.Models.ViewModels;
@using JijaShop.Models.DTOModels;
@using JijaShop.Models.Entities;
@using AutoMapper;

@model UserIndexViewModel

<div class="container">
	<div class="row">
		<div class="col-md-9" style="background-color: #D3D3D3;">
			<div class="row">
				@foreach (var product in await Model?.ProductService?.GetProducts())
				{
					<div class="col-4 text-center">
						<p>@product.Name</p>
						
					</div>
				}
			</div>
		</div>

		<div class="col-lg-3" style="background-color: whitesmoke">
			<div class="row">
				<div class="col-4">
					<p>Jija</p>
				</div>
			</div>
		</div>
	</div>
</div>